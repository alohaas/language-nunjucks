'fileTypes': [
  'nunjucks'
  'Nunjucks'
  'njk'
]
'firstLineMatch': '^{% extends ["\'][^"\']+["\'] %}'
'foldingStartMarker': '(<(?i:(head|table|tr|div|style|script|ul|ol|form|dl))\\b.*?>|{%\\s*(block|filter|for|if|macro|raw|asyncEach|asyncAll|call))'
'foldingStopMarker': '(</(?i:(head|table|tr|div|style|script|ul|ol|form|dl))\\b.*?>|{%\\s*(endblock|endfilter|endfor|endif|endmacro|endraw|endeach|endall|endcall)\\s*%})'
'name': 'HTML (Nunjucks Templates)'
'patterns': [
  {
    'include': 'source.nunjucks'
  }
  {
    'include': 'text.html.basic'
  }
]
'scopeName': 'text.html.nunjucks'
